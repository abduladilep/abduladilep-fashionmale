    <div class="container mt-5 mr-5">
        <div class="row tm-content-row">
            <div class="col-sm-12 col-md-12 col-lg-8 col-xl-8 tm-block-col">
                <div class="tm-bg-primary-dark tm-block tm-block-products">
                    <div class="tm-product-table-container text-white">
                        <div id="demo_info" class="box">
                            <table id="example" class="display table table-hover tm-table-small tm-product-table"
                                style="width:100%">
                                <thead>
                                    <tr>
                                        <th scope="col">&nbsp;</th>
                                        <th scope="col">FIRST NAME</th>
                                        <th scope="col">SECOND NAME</th>
                                        <th scope="col">Email</th>
                                        <th scope="col">BLOCK/UNBLOCK</th>
                                        <th scope="col">&nbsp;</th>
                                    </tr>
                                </thead>

                                <tbody>

                                    <% for(datas of showUser){ %>
                                        <tr>
                                            <th scope="row">
                                                <%= %>
                                            </th>
                                            <td>
                                                <%=datas.Firstname%>
                                            </td>
                                            <td>
                                                <%=datas.Lastname%>
                                            </td>
                                            <td>
                                                <%=datas.Email%>
                                            </td>

                                            <td>
                                                <% if(datas.state==false) {%>
                                                    <form action="/admin/editUser/<%= datas._id %>?_method=PUT"
                                                        method="post">
                                                        <button class="" type="submit"
                                                            onclick=" return confirm('Are u sure want to block <%=datas.Firstname%>')"
                                                            class="customButton-red">UNBLOCK</button>
                                                    </form>

                                                    <% }else{ %>

                                                        <form action="/admin/editUser/<%= datas._id %>?_method=PUT"
                                                            method="post">



                                                            <button class="customButton-red" type="submit"
                                                                onclick=" return confirm('Are u sure want to unblock <%= datas.Firstname%>')">BLOCK</button>

                                                        </form>
                                            </td>
                                            <% } %>
                                                </td>
                                                </td>
                                                <td></td>
                                        </tr>
                                        <%}%>
                                </tbody>
                        </div>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script src="js/jquery-3.3.1.min.js"></script>
    <!-- https://jquery.com/download/ -->
    <script src="js/bootstrap.min.js"></script>
    <!-- https://getbootstrap.com/ -->


    <script>
        $(function () {
            $(".tm-product-name").on("click", function () {
                window.location.href = "edit-product.html";
            });
        });
    </script>



    <script>
        $(document).ready(function () {
            var eventFired = function (type) {
                var n = $('#demo_info')[0];
                // n.innerHTML += '<div>' + type + ' event - ' + new Date().getTime() + '</div>';
                n.scrollTop = n.scrollHeight;
            };

            $('#example')
                .on('order.dt', function () {
                    eventFired('Order');
                })
                .on('search.dt', function () {
                    eventFired('Search');
                })
                .on('page.dt', function () {

                    eventFired('Page');
                })
                .DataTable();
        });
    </script>
